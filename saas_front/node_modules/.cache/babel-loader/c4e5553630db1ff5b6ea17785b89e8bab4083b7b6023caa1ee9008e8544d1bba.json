{"ast":null,"code":"import axios from \"axios\";\nimport dayjs from \"dayjs\";\nexport default {\n  data() {\n    return {\n      list: [],\n      // 分页相关\n      pageno: 1,\n      pagesize: 5,\n      total: 0,\n      // 搜索条件\n      params: {\n        accountingSubject: ''\n      }\n    };\n  },\n  created() {\n    this.getList();\n  },\n  methods: {\n    async getList() {\n      const token = localStorage.getItem(\"token\");\n      const res = await axios({\n        url: \"http://39.107.88.156:8080/sass_finance/accountBook/detailedAccountByPage\",\n        method: \"get\",\n        params: {\n          page: this.pageno,\n          limit: this.pagesize,\n          ...this.params\n        },\n        headers: {\n          'token': token\n        }\n      });\n      this.list = res.data.data;\n      this.total = res.data.count;\n    },\n    // 点击搜索\n    search() {\n      this.getList();\n    },\n    // 页码改变\n    pagechange(pageno) {\n      // 条件改变\n      this.pageno = pageno;\n      // 重新请求数据\n      this.getList();\n    },\n    formatDirection(row) {\n      // 根据 totalDebitAmount 的值来决定显示内容\n      if (row.debitAmount === 0) {\n        return '贷';\n      } else {\n        return '借';\n      }\n    },\n    formatDate(row) {\n      // 将时间戳转换为 Date 对象\n      const date = new Date(row.cime);\n      // 使用 Date 对象的方法获取年月日时分秒\n      const year = date.getFullYear();\n      const month = ('0' + (date.getMonth() + 1)).slice(-2);\n      const day = ('0' + date.getDate()).slice(-2);\n      const hours = ('0' + date.getHours()).slice(-2);\n      const minutes = ('0' + date.getMinutes()).slice(-2);\n      const seconds = ('0' + date.getSeconds()).slice(-2);\n\n      // 返回格式化后的日期字符串\n      return `${year}-${month}-${day} ${hours}:${minutes}:${seconds}`;\n    }\n  }\n};","map":{"version":3,"names":["axios","dayjs","data","list","pageno","pagesize","total","params","accountingSubject","created","getList","methods","token","localStorage","getItem","res","url","method","page","limit","headers","count","search","pagechange","formatDirection","row","debitAmount","formatDate","date","Date","cime","year","getFullYear","month","getMonth","slice","day","getDate","hours","getHours","minutes","getMinutes","seconds","getSeconds"],"sources":["src/views/accountBooks/DetailedAccount.vue"],"sourcesContent":["<template>\r\n    <div style=\"height: auto; width: auto;\">\r\n\r\n        <!-- 搜素和新增区域 -->\r\n        <el-card>\r\n            <div style=\"display: flex; flex-direction: row;\">\r\n                <!-- 搜索区域 -->\r\n                <div style=\"flex: 8;\">\r\n                    <el-form :inline=\"true\">\r\n                        <el-form-item label=\"根据科目名称查询\">\r\n                          <el-input placeholder=\"请输入科目名称\" prefix-icon=\"el-icon-search\"\r\n                                    v-model=\"params.accountingSubject\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item>\r\n                            <el-button @click=\"search\" type=\"primary\" icon=\"el-icon-search\">搜索</el-button>\r\n                        </el-form-item>\r\n                    </el-form>\r\n                </div>\r\n            </div>\r\n        </el-card>\r\n\r\n        <!-- 科目汇总 -->\r\n        <el-card style=\"margin-top: 20px;\">\r\n\r\n            <el-card>\r\n                <span\r\n                    style=\"font-size: 16px;font-weight: bold;font-family: 'Times New Roman', Times, serif;\">科目汇总</span>\r\n            </el-card>\r\n\r\n            <el-table :data=\"list\" style=\"width: 100%\">\r\n              <el-table-column type=\"selection\" width=\"55\">\r\n              </el-table-column>\r\n              <el-table-column prop=\"voucherId\" label=\"序号\"> \r\n              </el-table-column>\r\n              <el-table-column prop=\"cime\" label=\"日期\" :formatter=\"formatDate\"> \r\n              </el-table-column>\r\n              <el-table-column label=\"凭证号\">\r\n                <template slot-scope=\"scope\">\r\n                  {{ scope.row.voucherWord +\"-\"+ scope.row.voucherNumber }}\r\n                </template>\r\n              </el-table-column>\r\n              <el-table-column prop=\"accountingSubject\" label=\"科目\"> </el-table-column>\r\n                <el-table-column prop=\"debitAmount\" label=\"借方\"> </el-table-column>\r\n                <el-table-column prop=\"creditAmount\" label=\"贷方\"> </el-table-column>\r\n                <el-table-column label=\"方向\" :formatter=\"formatDirection\"> </el-table-column>\r\n                <!-- <el-table-column prop=\"tenantId\" label=\"租户id\"> </el-table-column> -->\r\n                <!-- <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"200px\">\r\n                  <template #default=\"{ row }\">\r\n                    <el-button type=\"primary\" icon=\"el-icon-edit\" circle @click=\"update(row)\"></el-button>\r\n                    <el-button type=\"danger\" icon=\"el-icon-delete\" circle @click=\"out(row)\"></el-button>\r\n                  </template>\r\n                </el-table-column> -->\r\n            </el-table>\r\n\r\n            <!-- 分页组件 -->\r\n            <el-pagination @current-change=\"pagechange\" background layout=\"prev, pager, next\" :total=\"total\"\r\n                :page-size=\"pagesize\" style=\"text-align: center; margin-top: 10px;\">\r\n            </el-pagination>\r\n        </el-card>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\nimport dayjs from \"dayjs\"\r\nexport default {\r\n\r\n    data() {\r\n        return {\r\n          list: [],\r\n          // 分页相关\r\n          pageno: 1,\r\n          pagesize: 5,\r\n          total: 0,\r\n          // 搜索条件\r\n          params: {\r\n            accountingSubject:''\r\n          }\r\n        };\r\n    },\r\n    created() {\r\n        this.getList()\r\n    },\r\n\r\n    methods: {\r\n        async getList() {\r\n          const token = localStorage.getItem(\"token\");\r\n            const res = await axios({\r\n                url: \"http://39.107.88.156:8080/sass_finance/accountBook/detailedAccountByPage\",\r\n                method: \"get\",\r\n                params: {\r\n                    page: this.pageno,\r\n                    limit: this.pagesize,\r\n                    ...this.params\r\n                }, \r\n                headers: {\r\n                    'token': token\r\n                }\r\n            });\r\n            this.list = res.data.data;\r\n            this.total = res.data.count;\r\n        },\r\n        // 点击搜索\r\n        search() {\r\n            this.getList()\r\n        },\r\n        // 页码改变\r\n        pagechange(pageno) {\r\n            // 条件改变\r\n            this.pageno = pageno\r\n            // 重新请求数据\r\n            this.getList()\r\n        },\r\n        formatDirection(row) {\r\n          // 根据 totalDebitAmount 的值来决定显示内容\r\n          if (row.debitAmount === 0) {\r\n            return '贷';\r\n          } else {\r\n            return '借';\r\n          }\r\n        },\r\n        formatDate(row) {\r\n          // 将时间戳转换为 Date 对象\r\n          const date = new Date(row.cime);\r\n          // 使用 Date 对象的方法获取年月日时分秒\r\n          const year = date.getFullYear();\r\n          const month = ('0' + (date.getMonth() + 1)).slice(-2);\r\n          const day = ('0' + date.getDate()).slice(-2);\r\n          const hours = ('0' + date.getHours()).slice(-2);\r\n          const minutes = ('0' + date.getMinutes()).slice(-2);\r\n          const seconds = ('0' + date.getSeconds()).slice(-2);\r\n          \r\n          // 返回格式化后的日期字符串\r\n          return `${year}-${month}-${day} ${hours}:${minutes}:${seconds}`;\r\n        }\r\n\r\n    },\r\n\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.row-box {\r\n    width: 100%;\r\n    display: flex;\r\n    margin-top: 20px;\r\n    margin-bottom: 8px;\r\n    justify-content: center;\r\n}\r\n</style>"],"mappings":"AAiEA,OAAAA,KAAA;AACA,OAAAC,KAAA;AACA;EAEAC,KAAA;IACA;MACAC,IAAA;MACA;MACAC,