{"ast":null,"code":"import axios from \"axios\";\nimport dayjs from \"dayjs\";\nexport default {\n  data() {\n    return {\n      list: [],\n      // 分页相关\n      pageno: 1,\n      pagesize: 5,\n      total: 0,\n      // 搜索条件\n      params: {\n        paymentProject: '',\n        state: null\n      },\n      // 新增/修改 内容\n      userFormData: {\n        companyRatio: '',\n        description: '',\n        paymentProject: '',\n        personalRatio: '',\n        id: '',\n        state: '',\n        tenantId: ''\n      },\n      show: false,\n      addshow: false\n    };\n  },\n  created() {\n    this.getList();\n  },\n  methods: {\n    async getList() {\n      const token = localStorage.getItem(\"token\");\n      const res = await axios({\n        url: \"http://localhost:8081/accumulation/list\",\n        method: \"get\",\n        params: {\n          page: this.pageno,\n          limit: this.pagesize,\n          ...this.params\n        },\n        headers: {\n          'token': token\n        }\n      });\n      this.list = res.data.data;\n      this.total = res.data.count;\n    },\n    // 点击搜索\n    search() {\n      this.getList();\n    },\n    // 页码改变\n    pagechange(pageno) {\n      // 条件改变\n      this.pageno = pageno;\n      // 重新请求数据\n      this.getList();\n    },\n    update(row) {\n      console.log(row);\n      this.show = true;\n      this.userFormData.id = row.id;\n      this.userFormData.paymentProject = row.paymentProject;\n      this.userFormData.companyRatio = row.companyRatio;\n      this.userFormData.personalRatio = row.personalRatio;\n      this.userFormData.description = row.description;\n      this.userFormData.state = row.state;\n      this.userFormData.tenantId = row.tenantId;\n    },\n    addopen() {\n      this.addshow = true;\n      this.userFormData.companyRatio = '', this.userFormData.description = '', this.userFormData.paymentProject = '', this.userFormData.personalRatio = '', this.userFormData.id = '', this.userFormData.state = 1, this.userFormData.tenantId = '';\n    },\n    cancel() {\n      this.show = false;\n      this.addshow = false;\n    },\n    async out(row) {\n      const data = {\n        id: row.id\n      };\n      const token = localStorage.getItem(\"token\");\n      const res = await axios({\n        url: \"http://localhost:8081/accumulation/updateStatus\",\n        method: \"post\",\n        params: data,\n        headers: {\n          'token': token\n        }\n      });\n      this.getList();\n    },\n    async submit() {\n      const data = {\n        ...this.userFormData\n      };\n      const token = localStorage.getItem(\"token\");\n      const res = await axios({\n        url: \"http://localhost:8081/accumulation/update\",\n        method: \"post\",\n        data: data,\n        headers: {\n          'token': token\n        }\n      });\n      this.getList();\n      this.show = false;\n    },\n    async addsubmit() {\n      const data = {\n        ...this.userFormData\n      };\n      const token = localStorage.getItem(\"token\");\n      const res = await axios({\n        url: \"http://localhost:8081/accumulation/add\",\n        method: \"post\",\n        data: data,\n        headers: {\n          'token': token\n        }\n      });\n      this.getList();\n      this.addshow = false;\n    }\n  }\n};","map":{"version":3,"names":["axios","dayjs","data","list","pageno","pagesize","total","params","paymentProject","state","userFormData","companyRatio","description","personalRatio","id","tenantId","show","addshow","created","getList","methods","token","localStorage","getItem","res","url","method","page","limit","headers","count","search","pagechange","update","row","console","log","addopen","cancel","out","submit","addsubmit"],"sources":["src/views/salary/BenefitsAndProvidentFund.vue"],"sourcesContent":["<template>\r\n    <div style=\"height: auto; width: auto;\">\r\n\r\n        <!-- 搜素和新增区域 -->\r\n        <el-card>\r\n            <div style=\"display: flex; flex-direction: row;\">\r