r\n            this.list = res.data.data;\r\n            this.total = res.data.count;\r\n        },\r\n        // 点击搜索\r\n        search() {\r\n            this.getList()\r\n        },\r\n        // 页码改变\r\n        pagechange(pageno) {\r\n            // 条件改变\r\n            this.pageno = pageno\r\n            // 重新请求数据\r\n            this.getList()\r\n        },\r\n        update(row) {\r\n          console.log(row)\r\n          this.show = true;\r\n          this.userFormData.id =  row.id;\r\n          this.userFormData.paymentProject = row.paymentProject;\r\n          this.userFormData.companyRatio = row.companyRatio;\r\n          this.userFormData.personalRatio = row.personalRatio;\r\n          this.userFormData.description = row.description;\r\n          this.userFormData.state = row.state;\r\n          this.userFormData.tenantId = row.tenantId;\r\n        },\r\n      addopen() {\r\n        this.addshow = true\r\n        this.userFormData.companyRatio =  '',\r\n        this.userFormData.description =  '',\r\n        this.userFormData.paymentProject =  '',\r\n        this.userFormData.personalRatio = '',\r\n        this.userFormData.id = '',\r\n        this.userFormData.state =  1,\r\n        this.userFormData.tenantId = ''\r\n      },\r\n      cancel() {\r\n        this.show = false\r\n        this.addshow = false\r\n      },\r\n      async out(row) {\r\n\r\n          const data = {\r\n            id: row.id,\r\n          }\r\n          const token = localStorage.getItem(\"token\");\r\n            const res = await axios({\r\n              url: \"http://localhost:8081/accumulation/updateStatus\",\r\n              method: \"post\",\r\n              params: data, \r\n                headers: {\r\n                    'token': token\r\n                }\r\n            })\r\n            this.getList();\r\n      },\r\n      async submit() {\r\n        const data = {\r\n          ...this.userFormData,\r\n        }\r\n        const token = localStorage.getItem(\"token\");\r\n        const res = await axios({\r\n          url: \"http://localhost:8081/accumulation/update\",\r\n          method: \"post\",\r\n          data: data, \r\n                headers: {\r\n                    'token': token\r\n                }\r\n        })\r\n        this.getList()\r\n        this.show = false\r\n      },\r\n      async addsubmit() {\r\n        const data = {\r\n          ...this.userFormData\r\n        }\r\n        const token = localStorage.getItem(\"token\");\r\n        const res = await axios({\r\n          url: \"http://localhost:8081/accumulation/add\",\r\n          method: \"post\",\r\n          data: data, \r\n                headers: {\r\n                    'token': token\r\n                }\r\n        })\r\n        this.getList()\r\n        this.addshow = false\r\n      }\r\n\r\n    },\r\n\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.row-box {\r\n    width: 100%;\r\n    display: flex;\r\n    margin-top: 20px;\r\n    margin-bottom: 8px;\r\n    justify-content: center;\r\n}\r\n</style>"],"mappings":"AAmJA,OAAAA,KAAA;AACA,OAAAC,KAAA;AACA;EAEAC,KAAA;IACA;MACAC,IAAA;MACA;MACAC,MAAA;MACAC,QAAA;MACAC,KAAA;MACA;MACAC,MAAA;QACAC,cAAA;QACAC,KAAA;MACA;MACA;MACAC,YAAA;QACAC,YAAA;QACAC,WAAA;QACAJ,cAAA;QACAK,aAAA;QACAC,EAAA;QACAL,KAAA;QACAM,QAAA;MACA;MACAC,IAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EAEAC,OAAA;IACA,MAAAD,QAAA;MACA,MAAAE,KAAA,GAAAC,YAAA,CAAAC,OAAA;MACA,MAAAC,GAAA,SAAAxB,KAAA;QACAyB,GAAA;QACAC,MAAA;QACAnB,MAAA;UACAoB,IAAA,OAAAvB,MAAA;UACAwB,KAAA,OAAAvB,QAAA;UACA,QAAAE;QACA;QACAsB,OAAA;UACA,SAAAR;QACA;MACA;MACA,KAAAlB,IAAA,GAAAqB,GAAA,CAAAtB,IAAA,CAAAA,IAAA;MACA,KAAAI,KAAA,GAAAkB,GAAA,CAAAtB,IAAA,CAAA4B,KAAA;IACA;IACA;IACAC,OAAA;MACA,KAAAZ,OAAA;IACA;IACA;IACAa,WAAA5B,MAAA;MACA;MACA,KAAAA,MAAA,GAAAA,MAAA;MACA;MACA,KAAAe,OAAA;IACA;IACAc,OAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA,KAAAlB,IAAA;MACA,KAAAN,YAAA,CAAAI,EAAA,GAAAoB,GAAA,CAAApB,EAAA;MACA,KAAAJ,YAAA,CAAAF,cAAA,GAAA0B,GAAA,CAAA1B,cAAA;MACA,KAAA