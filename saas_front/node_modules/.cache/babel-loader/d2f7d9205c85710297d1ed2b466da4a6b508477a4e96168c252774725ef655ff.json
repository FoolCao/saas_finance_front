      }, 1000);\n    },\n    async getSubjectAccountNameList() {\n      const res = await axios({\n        url: \"http://localhost:8081/bookkeeping/subjectAccountNameList\",\n        method: \"get\",\n        params: {\n          bookId: this.bookId\n          //bookId:6,\n        }\n      });\n      this.categoryList = res.data.data;\n    },\n    async getSummaryList() {\n      const res = await axios({\n        url: \"http://localhost:8081/bookkeeping/summaryList\",\n        method: \"get\",\n        params: {\n          bookId: this.bookId\n          //bookId:6,\n        }\n      });\n      this.summaryList = res.data.data;\n    },\n    async getClassificationList() {\n      const res = await axios({\n        url: \"http://localhost:8081/bookkeeping/classificationList\",\n        method: \"get\",\n        params: {\n          bookId: this.bookId\n          //bookId:6,\n        }\n      });\n      this.classificationList = res.data.data;\n    },\n    async getMakerList() {\n      const res = await axios({\n        url: \"http://localhost:8081/bookkeeping/makerList\",\n        method: \"get\",\n        params: {\n          bookId: this.bookId\n          //bookId:6,\n        }\n      });\n      this.makerList = res.data.data;\n    },\n    async getAccountingSubject() {\n      const res = await axios({\n        url: \"http://localhost:8081/bookkeeping/accountingSubjectList\",\n        method: \"get\",\n        params: {\n          bookId: this.bookId\n          //bookId:6,\n        }\n      });\n      this.accountingSubjectList = res.data.data;\n    },\n    async getList() {\n      const res = await axios({\n        url: \"http://localhost:8080/bookkeeping/findAll\",\n        method: \"get\",\n        params: {\n          ...this.params,\n          pageno: this.pageno,\n          pagesize: this.pagesize,\n          bookId: this.bookId\n          //bookId:6,\n        }\n      });\n      this.list = res.data.data.map(item => {\n        return {\n          ...item,\n          createTime: dayjs(item.createTime).format('YYYY-MM-DD '),\n          updateTime: dayjs(item.updateTime).format('YYYY-MM-DD ')\n        };\n      });\n      this.total = res.data.count;\n    },\n    // 点击搜索\n    search() {\n      this.getList();\n    },\n    // 页码改变\n    pagechange(pageno) {\n      // 条件改变\n      this.pageno = pageno;\n      // 重新请求数据\n      this.getList();\n    }\n  }\n};","map":{"version":3,"names":["axios","dayjs","Message","data","activeName","text","name","localStorage","getItem","summary","amount","accountingSubject","debitAmount","count","style","reason","note","tableHeaderClassName","list","categoryList","userFormData","date","format","category","remarks","pageno","pagesize","total","params","subjectAccountName","classification","maker","bookId","created","getSubjectAccountNameList","getSummaryList","getClassificationList","getMakerList","getAccountingSubject","getList","methods","handleClick","tab","event","console","log","updateApprovalStatus","row","reimbursementID","isToVoucher","res","url","method","setItem","success","setTimeout","$router","push","summaryList","classificationList","makerList","accountingSubjectList","map","item","createTime","updateTime","search","pagechange"],"sources":["src/views/bookkeeping/Stats.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <el-card>\r\n            <el-card>\r\n                <span style=\"font-size: 18px;font-weight: bold;font-family: 'Times New Roman', Times, serif;\">记账管理</span>\r\n            </el-card>\r\n            <el-card>\r\n                <el-tabs v-model=\"activeName\" @tab-click=\"handleClick\">\r\n                    <el-tab-pane label=\"账户汇总\" name=\"first\">\r\n                        <el-form :inline=\"true\">\r\n                            <el-form-item label=\"账户名称\">\r\n                                <el-select v-model=\"params.subjectAccountName\" class=\"filter-item\" placeholder=\"请选择\">\r\n                                    <el-option label=\"subjectA