{"ast":null,"code":"import axios from \"axios\";\nimport dayjs from \"dayjs\";\nimport { Table, TableColumn, Button, Dialog } from 'element-ui';\nexport default {\n  components: {\n    Table,\n    TableColumn,\n    Button,\n    Dialog\n  },\n  data() {\n    return {\n      tableData: [],\n      depreciationDetails: [],\n      dialogVisible: false,\n      params: {\n        name: '',\n        description: '',\n        typeId: ''\n      },\n      formData: {\n        name: '',\n        statue: '',\n        originalValue: '',\n        depreciationAmount: '',\n        description: '',\n        typeId: ''\n      },\n      page: 1,\n      limit: 5,\n      total: 0,\n      addshow: false,\n      typeList: [],\n      methodList: []\n    };\n  },\n  mounted() {\n    this.fetchData();\n    this.getType();\n  },\n  methods: {\n    async fetchData() {\n      const res = await axios({\n        method: \"get\",\n        url: \"http://localhost:8081/depreciation/page\",\n        params: {\n          page: this.page,\n          limit: this.limit,\n          ...this.params\n        }\n      });\n      this.tableData = res.data.data;\n      this.total = res.data.count;\n    },\n    showDepreciationDetails(row) {\n      this.depreciationDetails = row.depreciationDetailContentDepreciationDetailContentList;\n      this.dialogVisible = true;\n    },\n    async getType() {\n      const res = await axios({\n        method: \"get\",\n        url: \"http://localhost:8081/depreciationType/findAll\"\n      });\n      this.typeList = res.data.data;\n      this.typeList = this.typeList.filter(type => type.statue !== '禁用');\n      console.log(this.typeList);\n    },\n    // 点击搜索\n    search() {\n      this.fetchData();\n    }\n  }\n};","map":{"version":3,"names":["axios","dayjs","Table","TableColumn","Button","Dialog","components","data","tableData","depreciationDetails","dialogVisible","params","name","description","typeId","formData","statue","originalValue","depreciationAmount","page","limit",