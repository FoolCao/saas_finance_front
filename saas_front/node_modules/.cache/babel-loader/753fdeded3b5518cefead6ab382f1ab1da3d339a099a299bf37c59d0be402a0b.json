{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport dayjs from \"dayjs\";\nimport { Message } from 'element-ui';\nexport default {\n  data() {\n    return {\n      id: 0,\n      formData: {\n        number: '',\n        name: '',\n        depreciationMethod: {\n          id: '',\n          number: '',\n          depreciationName: '',\n          year: '',\n          term: '',\n          yearRate: '',\n          monthRate: '',\n          statue: '',\n          note: ''\n        },\n        statue: '',\n        note: ''\n      },\n      params: {\n        name: '',\n        number: '',\n        statue: '',\n        // 折旧方法 (搜索)\n        methodId: ''\n      },\n      AssetCategoryMethodList: [],\n      depreciationName: '',\n      multipleSelection: [],\n      // 折旧方法 (查找)\n      methodList: [],\n      selectedMethod: '',\n      // 分页\n      page: 1,\n      limit: 5,\n      total: 0,\n      addshow: false,\n      updateshow: false\n    };\n  },\n  created() {\n    this.DepreciationMethodgetList();\n    this.getMethod();\n  },\n  methods: {\n    async DepreciationMethodgetList() {\n      const res = await axios({\n        method: \"get\",\n        url: \"http://localhost:8081/depreciationType/page\",\n        params: {\n          page: this.page,\n          limit: this.limit,\n          ...this.params\n        }\n      });\n      this.AssetCategoryMethodList = res.data.data;\n      this.total = res.data.count;\n      console.log(res.data.data);\n    },\n    async getMethod() {\n      const res = await axios({\n        method: \"get\",\n        url: \"http://localhost:8081/depreciationMethod/findAll\"\n      });\n      this.methodList = res.data.data;\n      console.log(this.methodList);\n    },\n    async getMethodId() {\n      const selectedMethodObj = this.methodList.find(method => method.name === this.selectedMethod);\n      console.log(selectedMethodObj);\n      if (selectedMethodObj) {\n        this.formData.depreciationMethod.id = selectedMethodObj.id;\n      }\n      console.log(this.formData.depreciationMethod.id);\n    },\n    // 点击搜索\n    search() {\n      this.DepreciationMethodgetList();\n    },\n    cancel() {\n      this.updateshow = false;\n      this.addshow = false;\n    },\n    pagechange(page) {\n      // 条件改变\n      this.page = page;\n      // 重新请求数据\n      this.DepreciationMethodgetList();\n    },\n    async addsubmit() {\n      const data = {\n        ...this.formData\n      };\n      const res = await axios({\n        url: \"http://localhost:8081/depreciationMethod/add\",\n        method: \"post\",\n        data: data\n      });\n      this.DepreciationMethodgetList();\n      this.addshow = false;\n    },\n    addopen() {\n      this.addshow = true;\n    },\n    async remove(row) {\n      if (row.statue === '启用') {\n        this.$message.warning('不能删除启用项，请先设置为禁用');\n      } else {\n        this.$confirm('此操作将永久删除该文件, 是否继续?', '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'warning'\n        }).then(() => {\n          axios({\n            url: \"http://localhost:8081/depreciationMethod/delete/?id=\" + row.id,\n            method: \"delete\"\n          });\n          this.DepreciationMethodgetList();\n          this.$message({\n            type: 'success',\n            message: '删除成功!'\n          });\n        }).catch(() => {\n          this.$message({\n            type: 'info',\n            message: '已取消删除'\n          });\n        });\n      }\n    },\n    edit(row) {\n      this.updateshow = true;\n      this.formData.number = row.number;\n      this.formData.name = row.name;\n      this.formData.depreciationMethod.depreciationName = row.depreciationName;\n      this.formData.depreciationMethod.year = row.year;\n      this.formData.depreciationMethod.term = row.term;\n      this.formData.depreciationMethod.yearRate = row.yearRate;\n      this.formData.depreciationMethod.monthRate = row.monthRate;\n      this.formData.statue = row.statue;\n      this.formData.note = row.note;\n      this.id = row.id;\n    },\n    async updatesubmit() {\n      const data = {\n        id: this.id,\n        ...this.formData\n      };\n      const res = await axios({\n        url: \"http://localhost:8081/depreciationMethod/update\",\n        method: \"put\",\n        data: data\n      });\n      this.DepreciationMethodgetList();\n      this.updateshow = false;\n    },\n    handleSelectionChange(rows) {\n      this.multipleSelection = []; // 清空multipleSelection数组\n\n      rows.forEach(row => {\n        this.multipleSelection.push(row.id); // 将每行的id加入multipleSelection数组\n      });\n      console.log(this.multipleSelection); // 输出multipleSelection数组，包含所有选中行的id\n    },\n    batchdelete() {\n      const data = {\n        ids: this.multipleSelection\n      };\n      console.log(data);\n      this.$confirm('此操作将永久删除这些方法(不论方法是否启用!), 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        axios({\n          url: \"http://localhost:8081/depreciationMethod/delete/batch\",\n          method: \"delete\",\n          data: data\n        });\n        this.DepreciationMethodgetList();\n        this.$message({\n          type: 'success',\n          message: '删除成功!'\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消删除'\n        });\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","dayjs","Message","data","id","formData","number","name","depreciationMethod","depreciationName","year","term","yearRate","monthRate","statue","note","params","methodId","AssetCategoryMethodList","multipleSelection","methodList","selectedMethod","page","limit","total","addshow","updateshow","created","DepreciationMethodgetList","getMethod","methods","res","method","url","count","console","log","getMethodId","selectedMethodObj","find","search","cancel","pagechange","addsubmit","addopen","remove","row","$message","warning","$confirm","confirmButtonText","cancelButtonText","type","then","message","catch","edit","updatesubmit","handleSelectionChange","rows","forEach","push","batchdelete","ids"],"sources":["src/views/fixedasset/AssetCategory.vue"],"sourcesContent":["<template>\r\n    <div style=\"height: auto; width: auto;\">\r\n        <el-card>\r\n            <div style=\"display: flex; flex-direction: row;\">\r\n                <!-- 搜索区域 -->\r\n                <div style=\"flex: 8;\">\r\n                    <el-form :inline=\"true\">\r\n                        <el-form-item>\r\n                            <el-input v-model=\"params.name\" placeholder=\"请输入资产类别名称\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item>\r\n                            <el-input v-model=\"params.number\" placeholder=\"请输入资产类别编码\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item>\r\n                            <el-select v-model=\"params.statue\" placeholder=\"请选择状态\">\r\n                                <el-option value=\"\"></el-option>\r\n                                <el-option label=\"启用\" value=\"启用\"></el-option>\r\n                                <el-option label=\"禁用\" value=\"禁用\"></el-option>\r\n                            </el-select>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"折旧方法\">\r\n                            <el-select v-model=\"params.methodId\" placeholder=\"请选择折旧方法\" size=\"medium\"\r\n                                @change=\"getMethod\">\r\n                                <el-option value=\"\"></el-option>\r\n                                <el-option :label=\"method.depreciationName\" :value=\"method.id\" :key=\"method.id\"\r\n                                    v-for=\"method in methodList\" />\r\n                            </el-select>\r\n                        </el-form-item>\r\n                        <el-form-item>\r\n                            <el-button @click=\"search\" type=\"primary\" icon=\"el-icon-search\">查询</el-button>\r\n                        </el-form-item>\r\n                    </el-form>\r\n                </div>\r\n            </div>\r\n        </el-card>\r\n        <!-- 折旧方法 -->\r\n        <el-card style=\"margin-top: 20px;\">\r\n\r\n            <el-card>\r\n                <span\r\n                    style=\"font-size: 16px;font-weight: bold;font-family: 'Times New Roman', Times, serif;\">资产类别</span>\r\n                <div style=\"border: 2px; margin-top: 20px;\">\r\n                    <el-button type=\"primary\" @click=\"addopen\">新增</el-button>\r\n                    <el-button type=\"info\" plain @click=\"batchdelete\">批量删除</el-button>\r\n                    <el-button type=\"warning\" plain>启用</el-button>\r\n                    <el-button type=\"danger\" plain>禁用</el-button>\r\n                </div>\r\n            </el-card>\r\n\r\n            <el-table :data=\"AssetCategoryMethodList\" style=\"width: 100%\" @selection-change=\"handleSelectionChange\">\r\n\r\n                <el-table-column type=\"selection\" width=\"55\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"id\" label=\"序号\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"number\" label=\"资产类别编码\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"name\" label=\"资产类别名称\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"depreciationMethod.depreciationName\" label=\"折旧方法\" align=\"center\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"depreciationMethod.year\" label=\"预计使用年限\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"depreciationMethod.term\" label=\"使用月份\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"depreciationMethod.yearRate\" label=\"年折旧率\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"depreciationMethod.monthRate\" label=\"每期折旧率\" align=\"center\"></el-table-column>\r\n                <el-table-column prop=\"statue\" label=\"状态\" align=\"center\"></el-table-column>\r\n                <el-table-column prop=\"note\" label=\"备注\" align=\"center\"></el-table-column>\r\n                <el-table-column fixed=\"right\" label=\"操作\" width=\"150\" align=\"center\">\r\n                    <template #default=\"{ row }\">\r\n                        <el-button type=\"text\" size=\"small\" @click=\"remove(row)\">删除</el-button>\r\n                        <el-button type=\"text\" size=\"small\" @click=\"edit(row)\">修改</el-button>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n            <!-- 分页组件 -->\r\n            <el-pagination @current-change=\"pagechange\" background layout=\"prev, pager, next\" :total=\"total\"\r\n                :page-size=\"limit\" style=\"text-align: center; margin-top: 10px;\">\r\n            </el-pagination>\r\n        </el-card>\r\n\r\n        <!-- 新增弹窗区域 -->\r\n        <el-dialog title=\"新增资产类别\" :visible.sync=\"addshow\" width=\"600px\">\r\n            <el-form :model=\"formData\" class=\"demo-form-inline\" label-width=\"80px\">\r\n\r\n                <el-form-item label=\"资产类别编码\" label-width=\"120px\" :rules=\"[\r\n                        { required: true, message: '请输入资产类别编码', trigger: 'blur' }\r\n                    ]\">\r\n                    <el-input type=\"text\" v-model=\"formData.number\" placeholder=\"资产类别编码\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"资产类别名称\" label-width=\"120px\" :rules=\"[\r\n                        { required: true, message: '请输入资产类别名称', trigger: 'blur' }\r\n                    ]\">\r\n                    <el-input type=\"text\" v-model=\"formData.depreciationName\" placeholder=\"资产类别名称\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"折旧方法\">\r\n                    <el-select v-model=\"selectedMethod\" placeholder=\"选择折旧方法\" size=\"medium\" @change=\"getMethodId\">\r\n                        <el-option :label=\"method.depreciationName\" :value=\"method.id\" :key=\"method.id\"\r\n                            v-for=\"method in methodList\" />\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"预计使用年限\" label-width=\"100px\">\r\n                    <template>\r\n                        <el-input-number v-model=\"formData.year\" :step=\"2\" step-strictly></el-input-number>\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"使用月份\" label-width=\"100px\">\r\n                    <template>\r\n                        <el-input-number v-model=\"formData.term\" :step=\"2\" step-strictly></el-input-number>\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"年折旧率\" label-width=\"100px\">\r\n                    <template>\r\n                        <el-input-number v-model=\"formData.yearRate\" :precision=\"2\" :step=\"0.1\"\r\n                            :max=\"10\"></el-input-number>\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"每期折旧率\" label-width=\"100px\">\r\n                    <template>\r\n                        <el-input-number v-model=\"formData.monthRate\" :precision=\"2\" :step=\"0.1\"\r\n                            :max=\"10\"></el-input-number>\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"状态\" label-width=\"100px\">\r\n                    <el-radio-group v-model=\"formData.statue\">\r\n                        <el-radio label=\"启用\"></el-radio>\r\n                        <el-radio label=\"禁用\"></el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n                <el-form-item label=\"备注\" label-width=\"100px\">\r\n                    <el-input type=\"textarea\" v-model=\"formData.note\" placeholder=\"备注\"\r\n                        :autosize=\"{ minRows: 4, maxRows: 6 }\"></el-input>\r\n                </el-form-item>\r\n\r\n                <!-- 使用 Flex 布局排列按钮 -->\r\n                <el-form-item class=\"button-group\">\r\n                    <el-button @click=\"addsubmit\" type=\"primary\">提交</el-button>\r\n                    <el-button @click=\"cancel\" type=\"info\">取消</el-button>\r\n                </el-form-item>\r\n\r\n            </el-form>\r\n        </el-dialog>\r\n\r\n        <!-- 修改弹窗区域 -->\r\n        <el-dialog title=\"修改折旧方法\" :visible.sync=\"updateshow\" width=\"600px\">\r\n            <el-form :model=\"formData\" class=\"demo-form-inline\" label-width=\"80px\">\r\n\r\n                <el-form-item label=\"资产类别编码\" label-width=\"120px\" :rules=\"[\r\n                        { required: true, message: '请输入资产类别编码', trigger: 'blur' }\r\n                    ]\">\r\n                    <el-input type=\"text\" v-model=\"formData.number\" placeholder=\"资产类别编码\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"资产类别名称\" label-width=\"120px\" :rules=\"[\r\n                        { required: true, message: '请输入资产类别名称', trigger: 'blur' }\r\n                    ]\">\r\n                    <el-input type=\"text\" v-model=\"formData.name\" placeholder=\"资产类别名称\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"折旧方法\">\r\n                    <el-select v-model=\"selectedMethod\" placeholder=\"选择折旧方法\" size=\"medium\" @change=\"getMethodId\">\r\n                        <el-option :label=\"method.depreciationName\" :value=\"method.id\" :key=\"method.id\"\r\n                            v-for=\"method in methodList\" />\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"预计使用年限\" label-width=\"100px\">\r\n                    <template>\r\n                        <el-input-number v-model=\"formData.year\" :step=\"2\" step-strictly></el-input-number>\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"使用月份\" label-width=\"100px\">\r\n                    <template>\r\n                        <el-input-number v-model=\"formData.term\" :step=\"2\" step-strictly></el-input-number>\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"年折旧率\" label-width=\"100px\">\r\n                    <template>\r\n                        <el-input-number v-model=\"formData.yearRate\" :precision=\"2\" :step=\"0.1\"\r\n                            :max=\"10\"></el-input-number>\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"每期折旧率\" label-width=\"100px\">\r\n                    <template>\r\n                        <el-input-number v-model=\"formData.monthRate\" :precision=\"2\" :step=\"0.1\"\r\n                            :max=\"10\"></el-input-number>\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"状态\" label-width=\"100px\">\r\n                    <el-radio-group v-model=\"formData.statue\">\r\n                        <el-radio label=\"启用\"></el-radio>\r\n                        <el-radio label=\"禁用\"></el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n                <el-form-item label=\"备注\" label-width=\"100px\">\r\n                    <el-input type=\"textarea\" v-model=\"formData.note\" placeholder=\"备注\"\r\n                        :autosize=\"{ minRows: 4, maxRows: 6 }\"></el-input>\r\n                </el-form-item>\r\n\r\n                <!-- 使用 Flex 布局排列按钮 -->\r\n                <el-form-item class=\"button-group\">\r\n                    <el-button @click=\"addsubmit\" type=\"primary\">提交</el-button>\r\n                    <el-button @click=\"cancel\" type=\"info\">取消</el-button>\r\n                </el-form-item>\r\n\r\n            </el-form>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n<script>\r\n\r\nimport axios from \"axios\";\r\nimport dayjs from \"dayjs\";\r\nimport { Message } from 'element-ui'\r\nexport default {\r\n    data() {\r\n        return {\r\n            id: 0,\r\n            formData: {\r\n                number: '',\r\n                name: '',\r\n                depreciationMethod: {\r\n                    id: '',\r\n                    number: '',\r\n                    depreciationName: '',\r\n                    year: '',\r\n                    term: '',\r\n                    yearRate: '',\r\n                    monthRate: '',\r\n                    statue: '',\r\n                    note: ''\r\n                },\r\n                statue: '',\r\n                note: ''\r\n            },\r\n            params: {\r\n                name: '',\r\n                number: '',\r\n                statue: '',\r\n                // 折旧方法 (搜索)\r\n                methodId: ''\r\n            },\r\n            AssetCategoryMethodList: [],\r\n            depreciationName: '',\r\n            multipleSelection: [],\r\n            // 折旧方法 (查找)\r\n            methodList: [],\r\n            selectedMethod: '',\r\n            // 分页\r\n            page: 1,\r\n            limit: 5,\r\n            total: 0,\r\n            addshow: false,\r\n            updateshow: false\r\n        }\r\n    },\r\n    created() {\r\n        this.DepreciationMethodgetList()\r\n        this.getMethod()\r\n    },\r\n    methods: {\r\n        async DepreciationMethodgetList() {\r\n            const res = await axios({\r\n                method: \"get\",\r\n                url: \"http://localhost:8081/depreciationType/page\",\r\n                params: {\r\n                    page: this.p